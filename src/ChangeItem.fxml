<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="350.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17.0.8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ChangeItemController">
   <children>
      <Label layoutX="117.0" layoutY="25.0" text="Item Details:">
         <font>
            <Font name="Felix Titling" size="24.0" />
         </font>
      </Label>
      <Label layoutX="44.0" layoutY="90.0" text="Name">
         <font>
            <Font name="Felix Titling" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="nameField" layoutX="128.0" layoutY="90.0" prefHeight="23.0" prefWidth="241.0" promptText="Name">
         <font>
            <Font name="Felix Titling" size="12.0" />
         </font>
      </TextField>
      <Label layoutX="44.0" layoutY="125.0" text="Price">
         <font>
            <Font name="Felix Titling" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="priceField" layoutX="128.0" layoutY="125.0" prefHeight="23.0" prefWidth="241.0" promptText="Price">
         <font>
            <Font name="Felix Titling" size="12.0" />
         </font>
      </TextField>
      <Label alignment="CENTER" layoutX="26.0" layoutY="163.0" text="Calories">
         <font>
            <Font name="Felix Titling" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="caloriesField" layoutX="128.0" layoutY="163.0" prefHeight="23.0" prefWidth="241.0" promptText="Calories">
         <font>
            <Font name="Felix Titling" size="12.0" />
         </font>
      </TextField>
      <Label alignment="CENTER" layoutX="26.0" layoutY="200.0" text="Quantity">
         <font>
            <Font name="Felix Titling" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="quantityField" layoutX="128.0" layoutY="200.0" prefHeight="23.0" prefWidth="241.0" promptText="Quantity">
         <font>
            <Font name="Felix Titling" size="12.0" />
         </font>
      </TextField>
      <Label alignment="CENTER" layoutX="48.0" layoutY="239.0" text="Slot">
         <font>
            <Font name="Felix Titling" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="slotField" layoutX="128.0" layoutY="239.0" prefHeight="23.0" prefWidth="241.0" promptText="Slot">
         <font>
            <Font name="Felix Titling" size="12.0" />
         </font>
      </TextField>
      <Button layoutX="164.0" layoutY="286.0" mnemonicParsing="false" onAction="#handleSubmission" text="Save">
         <font>
            <Font name="Felix Titling" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
